  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.11.29",
    "@tsconfig/node18": "^18.2.4",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "@types/pg": "^8.15.2",
    "eslint": "^9.27.0",
    "eslint-plugin-import": "^2.31.0",
